@{
    ViewData["Title"] = "Dashboard";
    Layout = "_Layout";
}
<div class="content">
    <div class="row">
        <div class="col-xl-7">
            <div class="card">
                <div class="card-body py-0">
                    <div class="row">
                        <div class="chart col-md-12" id="barCharts">
                        </div>
                    </div>
                </div>

            </div>
        </div>

        <div class="col-xl-5">
            <div class="card">
                <div class="chart col-md-12" id="pieCharts">
                </div>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-xl-7">
            <div class="card">
                <div class="card-body py-0">
                    <div class="row">
                        <div class="chart col-md-12" id="lineCharts">
                        </div>
                    </div>
                </div>

            </div>
        </div>

        <div class="col-xl-5">
            <div class="card">
                <div class="chart col-md-12" id="container">
                </div>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-xl-7">
            <div class="card">
                <div class="card-body py-0">
                    <div class="row">
                        <div class="chart col-md-12" id="chartcontainer">
                        </div>
                    </div>
                </div>

            </div>
        </div>

        <div class="col-xl-5">
            <div class="card">
                <div class="chart col-md-12" id="containerchart2">
                </div>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-xl-7">
            <div class="card">
                <div class="card-body py-0">
                    <div class="row">
                        <div class="chart col-md-12" id="containerkenya">
                        </div>
                    </div>
                </div>

            </div>
        </div>

        <div class="col-xl-5">
            <div class="card">
                <div class="chart col-md-12" id="container3">
                </div>
            </div>
        </div>
    </div>
</div>


